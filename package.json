{
  "name": "react-functional-router",
  "version": "0.2.3",
  "description": "A very simple router for react with a functional api",
  "main": "index.js",
  "scripts": {
    "test": "mocha 'src/**/test*.coffee' --watch --compilers coffee:coffeescript/register",
    "clean": "rimraf *.js",
    "build": "coffee -o ./ -c src/",
    "dist": "npm run clean && npm run build",
    "pub": "npm run dist && npm publish && npm run clean"
  },
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "ramda": "^0.22.1",
    "ramda-extras": "^0.4.13",
    "react": "^16.13.0"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.2.3",
    "dotenv": "^5.0.1",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.10.0",
    "rimraf": "^2.4.3",
    "testhelp": "^0.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cottin/react-functional-router.git"
  },
  "keywords": [
    "react",
    "router",
    "routing"
  ],
  "author": "Victor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cottin/react-functional-router/issues"
  },
  "homepage": "https://github.com/Cottin/react-functional-router#readme"
}
