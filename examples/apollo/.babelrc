{
  "presets": [
    [
      "env",
      {
        "loose": true,
        "modules": false,
        "targets": {
          "browsers": "last 2 chrome versions"
        },
        include: ['transform-es2015-classes']
      }
    ],
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["transform-class-properties", { "loose": true }]
  ]
}
